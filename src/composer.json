    "require": {
        "php": ">=8.0.0",
        "propel/propel": "2.0.0-beta4",
        "drewm/mailchimp-api": "v2.5.4",
        "slim/slim": "4.*",
        "slim/php-view": "3.2.0",
        "slim/http-cache": "^1.2.0",
        "slim/psr7": "^1.7.1",
        "slim/http": "^1.4.0",
        "sabre/dav": "4.*",
        "monolog/monolog": "^1.17",
        "authorizenet/authorizenet": "1.9.4",
        "phpmailer/phpmailer":  "v6.5.0",
        "verot/class.upload.php": "dev-master",
        "ifsnop/mysqldump-php": "2.*",
        "php-http/message": "1.6.0",
        "geocoder-php/google-maps-provider": "4.*",
        "geocoder-php/bing-maps-provider": "4.*",
        "geocoder-php/nominatim-provider": "5.*",
        "mustache/mustache": "~2.14.2",
        "phpoffice/phpword": "^1.4.0",
        "phpoffice/phpspreadsheet": "^1.29.11",
        "defuse/php-encryption": "^2.4.0",
        "endroid/qr-code": "4.*",
        "ext-gettext": "*",
        "laminas/laminas-escaper": "2.*",
        "php-http/curl-client": "^2.3.3",
        "knplabs/github-api": "^3.16.0",
        "guzzlehttp/guzzle": "^7.9.3",
        "http-interop/http-factory-guzzle": "^1.2.0",
        "php-di/php-di": "7.0.*",
        "tecnickcom/tcpdf": "^6.10.0",
        "robthree/twofactorauth": "^1.8.2",
        "jimtools/jwt-auth": "^1.1.0"
    },
    "autoload": {
        "classmap": [
            "EcclesiaCRM/",
            "EcclesiaCRM/sabre"
        ],
        "psr-4": {
            "\\": "bin/",
            "EcclesiaCRM\\": "EcclesiaCRM/",
            "PluginStore\\": "Plugins/"
        }
    },
    "repositories": {
        "packagist": {
            "url": "https://packagist.org",
            "type": "composer"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
